{
  "expo": {
    "name": "LiftLog",
    "slug": "liftlog",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "splash": {
      "backgroundColor": "#E0EDDB",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "backgroundColor": "#E0EDDB",
    "primaryColor": "#046F03",
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "app",
          "organization": "liftlog"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "scheme": "app.liftlog.online",
    "android": {
      "package": "com.limajuice.liftlog",
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.SCHEDULE_EXACT_ALARM"]
    },
    "ios": {
      "bundleIdentifier": "com.limajuice.liftlog",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true
      },
      "appleTeamId": "DM9JP7639R"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "jsEngine": "hermes"
  }
}
