{
  "name": "liftlog-react",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "prebuild": "expo prebuild",
    "build:android:release": "./android/gradlew assembleRelease",
    "build:web": "expo export -p web --source-maps",
    "android": "expo run:android",
    "android:release": "expo run:android --variant release",
    "ios": "expo run:ios",
    "dev": "expo start --dev-client",
    "web": "expo start --web",
    "lint": "eslint .",
    "patch-package": "patch-package",
    "postinstall": "npm run patch-package",
    "protojs": "pbjs -p ../proto  -t static-module ../proto/**/*.proto ../proto/SharedItem.proto -o ./gen/proto.js -w es6 --es6 --force-long",
    "protots": "pbts -o ./gen/proto.d.ts ./gen/proto.js",
    "proto": "npm run protojs && npm run protots",
    "pull-translations": "tolgee  --api-key ${TOLGEE_PAT} pull --path ./i18n ",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
