{
  "name": "liftlog-react",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:release": "expo run:android --variant release",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "patch-package": "patch-package",
    "postinstall": "npm run patch-package",
    "protojs": "pbjs -p ../proto  -t static-module ../proto/**/*.proto -o ./gen/proto.js -w es6 --es6 --force-long",
    "protots": "pbts -o ./gen/proto.d.ts ./gen/proto.js",
    "proto": "npm run protojs && npm run protots",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@js-joda/core": "^5.6.4",
    "@pchmn/expo-material3-theme": "^1.3.2",
    "@react-navigation/elements": "^2.2.6",
    "@reduxjs/toolkit": "^2.6.1",
    "@tolgee/react": "^6.2.2",
    "babel-plugin-react-compiler": "^19.0.0-beta-bafa41b-20250307",
    "bignumber.js": "^9.1.2",
    "data-async-iterators": "^1.4.6",
    "eslint-plugin-react-compiler": "^19.0.0-beta-bafa41b-20250307",
    "expo": "~52.0.46",
    "expo-constants": "~17.0.3",
    "expo-file-system": "~18.0.12",
    "expo-intent-launcher": "~12.0.2",
    "expo-linking": "~7.0.3",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.0",
    "linq": "^4.0.3",
    "protobufjs": "^7.5.0",
    "react": "18.3.1",
    "react-compiler-runtime": "^19.0.0-beta-bafa41b-20250307",
    "react-native": "0.76.9",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-paper": "^5.13.1",
    "react-native-permissions": "^5.3.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-redux": "^9.2.0",
    "ts-pattern": "^5.6.2",
    "use-debounce": "^10.0.4",
