    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fast-check": "^4.1.1",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.3",
    "protobufjs-cli": "^1.1.3",
    "typescript": "~5.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
